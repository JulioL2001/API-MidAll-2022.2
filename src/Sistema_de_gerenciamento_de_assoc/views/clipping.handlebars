<div class="container">
    <nav>
        <ul>
            <img src="../assets/img/MIDALL NOVOb.png" class="imagem">
            <li><a href="/sistema/clipping">Clipping de Email</a></li>
            <li><a href="/sistema/gerenc_assoc">Gerenciar Associados</a></li>
            <li><a href="/sistema/cadastro_assoc">Cadastrar Associados</a></li>
            <img src="../assets/img/MIDALL NOVOb.png" class="imagem1">
        </ul>
    </nav>

    <section class="clipping_emails">
        <div class="coluna">
            <h1 class="gradient">Clipping de Email</h1>
            <div class="borda">
                <form action="/sistema/alt_email" method="post"></form>
                <table class="dados1">
                     <tr class="gradient">
                            <th>    </th>
                            <th>Nome</th>
                            <th>Email</th>
                        </tr>
                    {{#each Assoc}}
                    <label for="{{id}}"></label>
                    <div class="dados">
                        <tr>
                        <td><input type="checkbox" id="{{id}}" value="{{id}}"></td>
                        <div class="nome_as">
                            <td><p>{{nome}}</p></td>
                        </div>
                         <div class="ema">
                            <td><p>{{email}}</p></td>
                        </div>
                        </tr>
                    </div>
                     </label>
                    {{/each}}
                </table>
            </div>
            <div class="align_buts">
                <button type="submit">Alterar</button>
            </div>
            </form>
        </div>
    </section>
</div>